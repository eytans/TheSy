(include nat)
(include nat.res)

fun tri (_: Nat) -> Nat

rw tri_base (tri zero) => zero
rw tri_ind_1 (tri (succ ?n)) => (plus (tri ?n) (succ ?n))
rw tri_ind_2 (plus (tri ?n) (succ ?n)) => (tri (succ ?n))

fun cubes (_: Nat) -> Nat

rw cubes_base (cubes zero) => zero
rw cubes_ind_1 (cubes (succ ?n)) => (plus (cubes ?n) (mult (succ ?n) (mult (succ ?n) (succ ?n))))
rw cubes_ind_2 (plus (cubes ?n) (mult (succ ?n) (mult (succ ?n) (succ ?n)))) => (cubes (succ ?n))

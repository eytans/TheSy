(datatype Nat () ((Z Nat) (S Nat Nat)))
(declare-fun <2 (Nat Nat) Bool)
(declare-fun +2 (Nat Nat) Nat)
(=> rule0 (<2 ?x Z) false)
(=> rule1 (<2 Z (S ?z)) true)
(=> rule2 (<2 (S ?x2) (S ?z)) (<2 ?x2 ?z))
(=> rule3 (<2 ?x2 ?z) (<2 (S ?x2) (S ?z)))
(=> rule4 (+2 Z ?y) ?y)
(=> rule5 (+2 (S ?z) ?y) (S (+2 ?z ?y)))
(=> rule6 (S (+2 ?z ?y)) (+2 (S ?z) ?y))
(prove (forall ((i Nat)(m Nat)) (= (<2 i (S (+2 m i))) true)))

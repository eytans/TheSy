(datatype list () ((nil list) (cons sk list list)))
(datatype Nat () ((Z Nat) (S Nat Nat)))
(declare-fun drop (Nat list) list)
(=> rule0 (drop Z ?y) ?y)
(=> rule1 (drop (S ?z) nil) nil)
(=> rule2 (drop (S ?z) (cons ?x2 ?x3)) (drop ?z ?x3))
(prove (forall ((xs list)(n Nat)(x sk)) (= (drop (S n) (cons x xs)) (drop n xs))))

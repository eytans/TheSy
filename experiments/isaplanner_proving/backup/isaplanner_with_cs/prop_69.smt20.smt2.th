(datatype Nat () ((Z Nat) (S Nat Nat)))
(declare-fun <=2 (Nat Nat) Bool)
(declare-fun +2 (Nat Nat) Nat)
(=> rule0 (<=2 Z ?y) true)
(=> rule1 (<=2 (S ?z) Z) false)
(=> rule2 (<=2 (S ?z) (S ?x2)) (<=2 ?z ?x2))
(=> rule3 (<=2 ?z ?x2) (<=2 (S ?z) (S ?x2)))
(=> rule4 (+2 Z ?y) ?y)
(=> rule5 (+2 (S ?z) ?y) (S (+2 ?z ?y)))
(=> rule6 (S (+2 ?z ?y)) (+2 (S ?z) ?y))
(prove (forall ((n Nat)(m Nat)) (= (<=2 n (+2 m n)) true)))

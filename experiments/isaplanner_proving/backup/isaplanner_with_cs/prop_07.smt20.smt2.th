(datatype Nat () ((Z Nat) (S Nat Nat)))
(declare-fun -2 (Nat Nat) Nat)
(declare-fun +2 (Nat Nat) Nat)
(=> rule0 (-2 Z ?y) Z)
(=> rule1 (-2 (S ?z) Z) (S ?z))
(=> rule2 (S ?z) (-2 (S ?z) Z))
(=> rule3 (-2 (S ?z) (S ?x2)) (-2 ?z ?x2))
(=> rule4 (-2 ?z ?x2) (-2 (S ?z) (S ?x2)))
(=> rule5 (+2 Z ?y) ?y)
(=> rule6 (+2 (S ?z) ?y) (S (+2 ?z ?y)))
(=> rule7 (S (+2 ?z ?y)) (+2 (S ?z) ?y))
(prove (forall ((n Nat)(m Nat)) (= (-2 (+2 n m) n) m)))
